{
    "thumbNailsExec": [
        "echo \"create: %count --> %out\"",
        "/usr/bin/convert -auto-orient -thumbnail 200 \"%in\" \"%out\"",
        "sleep 0.1"
    ],
    "thumbNailsExecFile": "../createTn.sh",
    "thumbNailsRoot": "../thumbnailsNew",
    "thumbNailTimeStamp": "%y_%m_%d_%H_%M_%S_",
    "thumbNailFileSuffix": ".jpg",
    "imageExtensions": [
        ".jpg"
    ],
    "logPath": "../thumbnailLogs",
    "logName": "tnLog_%y_%m_%d_%H_%M_%S.log",
    "verbose": false,
    "resources": {
        "stuart": {
            "imageRoot": "/media/USBHDD1/shares",
            "imagePaths": [
                "PixelPhoneSync"
            ]
        }
    }
}
